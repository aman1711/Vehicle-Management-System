{
  "name": "@mysql/xdevapi",
  "version": "8.0.21",
  "description": "MySQL Connector/Node.js - A Node.js driver for MySQL using the X Protocol and X DevAPI.",
  "contributors": [
    {
      "name": "Johannes SchlÃ¼ter",
      "email": "johannes.schlueter@oracle.com"
    },
    {
      "name": "Rui Quelhas",
